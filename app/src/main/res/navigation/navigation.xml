<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@id/initialFragment">

    <fragment
        android:id="@+id/initialFragment"
        android:name="com.example.at1.View.InitialFragment"
        android:label="fragment_initial"
        tools:layout="@layout/fragment_initial" >
        <action
            android:id="@+id/action_initialFragment_to_faleConoscoFragment3"
            app:destination="@id/faleConoscoFragment" />
        <action
            android:id="@+id/action_initialFragment_to_sobreFragment"
            app:destination="@id/sobreFragment" />
    </fragment>
    <fragment
        android:id="@+id/sobreFragment"
        android:name="com.example.at1.View.SobreFragment"
        android:label="fragment_sobre"
        tools:layout="@layout/fragment_sobre" />
    <fragment
        android:id="@+id/faleConoscoFragment"
        android:name="com.example.at1.View.FaleConoscoFragment"
        android:label="fragment_fale_conosco"
        tools:layout="@layout/fragment_fale_conosco" />
</navigation>